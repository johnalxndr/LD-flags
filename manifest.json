{
  "manifest_version": 3,
  "name": "Feature Flags Viewer",
  "version": "1.0",
  "description": "Displays the response of LaunchDarkly XHR request for active feature flags.",
  "permissions": [
    "activeTab",
    "webRequest",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://app.launchdarkly.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
